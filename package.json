{
  "name": "library-app",
  "version": "3.19.0",
  "description": "Demo application for learning Ember.js v3 (Octane)",
  "repository": {
    "type": "git",
    "url": "https://github.com/zoltan-nz/library-app"
  },
  "license": "MIT",
  "author": "Zoltan Debre",
  "private": false,
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "run-p --aggregate-output -c -l lint:hbs lint:js",
    "lint:hbs": "ember-template-lint . --fix",
    "lint:js": "eslint . --fix",
    "lint:js:check": "eslint .",
    "start": "ember serve",
    "test": "run-p --aggregate-output -l lint test:ember",
    "test:ember": "ember test",
    "test:coverage": "COVERAGE=true ember test",
    "format": "prettier --list-different --write **/*.{js,html,json,css,scss,md}",
    "format:check": "prettier --check **/*.{js,html,json,css,scss,md}",
    "ci:report:codacy": "cat ./coverage/lcov.info | codacy-coverage",
    "ci:report:coveralls": "cat ./coverage/lcov.info | coveralls",
    "ci": "run-s -l lint:js:check format:check test:coverage"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@fortawesome/ember-fontawesome": "^0.2.1",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@glimmer/component": "^1.0.2",
    "@glimmer/tracking": "^1.0.2",
    "babel-eslint": "^10.1.0",
    "bootstrap": "^4.5.2",
    "broccoli-asset-rev": "^3.0.0",
    "codacy-coverage": "^3.4.0",
    "coveralls": "^3.1.0",
    "ember-auto-import": "^1.6.0",
    "ember-bootstrap": "^4.1.0",
    "ember-cli": "~3.22.0",
    "ember-cli-app-version": "^4.0.0",
    "ember-cli-babel": "^7.22.1",
    "ember-cli-code-coverage": "^1.0.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-dotenv": "^3.1.0",
    "ember-cli-htmlbars": "^5.3.1",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-sass": "^10.0.1",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.0",
    "ember-cli-update": "^0.54.6",
    "ember-concurrency": "^1.2.1",
    "ember-concurrency-decorators": "^2.0.1",
    "ember-data": "~3.22.0",
    "ember-export-application-global": "^2.0.1",
    "ember-faker": "^1.5.0",
    "ember-fetch": "^8.0.2",
    "ember-load-initializers": "^2.1.1",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^4.6.0",
    "ember-resolver": "^8.0.2",
    "ember-sinon": "^5.0.0",
    "ember-source": "~3.22.0",
    "ember-template-lint": "^2.14.0",
    "ember-welcome-page": "^4.0.0",
    "emberfire": "^3.0.0-rc.6",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-ember": "^9.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "firebase": "^7.19.1",
    "husky": "^4.2.5",
    "loader.js": "^4.7.0",
    "lodash": "^4.17.20",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.1",
    "qunit-dom": "^1.5.0",
    "sass": "^1.26.10"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember": {
    "edition": "octane"
  },
  "husky": {
    "hooks": {
      "pre-push": "run-p lint:js:check format:check test:ember"
    }
  }
}
